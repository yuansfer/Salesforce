<?xml version="1.0" encoding="ISO-8859-1" ?>
<extensions xmlns="http://www.demandware.com/xml/bmmodules/2007-12-11"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.demandware.com/xml/bmmodules/2007-12-11 bmext.xsd">

	<menuitem id="yuansfer_site" position="9999" site="true">
		<name xml:lang="x-default">Yuansfer</name>
		<short_description xml:lang="x-default">Yuansfer</short_description>
		<description xml:lang="x-default">Yuansfer</description>
 		<icon>icons/channel_ico.gif</icon>
	</menuitem>

	<menuaction id="yuansfer_site_order" menupath="yuansfer_site" position="9999" site="true">
		<name xml:lang="x-default">Yuansfer transactions</name>
		<short_description xml:lang="x-default">
			Manage the Yuansfer transactions
		</short_description>
		<description xml:lang="x-default">
			Manage the Yuansfer transactions
		</description>
		<exec pipeline="YuansferBM" node="ListTransactions" />
		<sub-pipelines>
			<pipeline name="YuansferBM" />
		</sub-pipelines>
		<icon>icons/sitespaymentmethods_ico.gif</icon>
	</menuaction>
</extensions>
